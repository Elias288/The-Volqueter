function player_animate()
	if player.junmping or player.falling then
		player.anim=2
		player.anim_c=0
	elseif player.running then
		if time()-player.time>.1 then
			player.time=time()
			
			player.anim+=2
			player.anim_c+=2
			if player.anim>2 then
				player.anim=0
				player.anim_c=0
			end
		end
	elseif player.sliding then
		player.anim=0
		player.anim_c=0
	elseif player.attack then
		if tiempo%6==0 then
			player.anim+=2
			if player.anim>6 then
				player.anim=player.sp_attack
			end
		end
	end
end

function monster_animate(a)
	if a.attack then
		if tiempo%6==0 then
			a.anim+=2
			if a.anim>6 then
				a.anim=a.sp_attack
			end
		end
		
	elseif a.running then
		if time()-a.time>.1 then
			a.time=time()
			a.anim+=2
			if a.anim>2 then
				a.anim=0
			end
		end
	end
end

