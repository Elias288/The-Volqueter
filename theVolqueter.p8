function update_game()
	tiempo += 1

	foreach(enemigos, monster_animate)
	foreach(items, item_update)
	foreach(carteles, carteles_update)

	player_update()
	player_animate()

	cam_x=player.x-64+(player.w/2)
	if cam_x<map_start then
		cam_x=map_start
	end
	
	if cam_x>map_end-128 then
		cam_x=map_end-128
	end

	camera(cam_x,0)

end
function player_update()
	--physics
	player.dy+=gravity
	player.dx*=friccion


	if player.vida>0 then
		--controls
		if btn(➡️) then
			player.dx+=player.acc
			player.running=true
			player.flp=false
		end
		if btn(❎) and player.landed then
			player.dy-=player.boost
			player.landed=false
		end
		if btn(🅾️) then
			player.attack=true
			player.anim=player.sp_attack
			tiempo=0
		else
			player.attack=false
		end

		if btn(⬅️) then
			player.dx-=player.acc
			player.flp=true
			player.running=true
		end

	end

	if tiempo==15 then
		player.attack=false
	end
	
	if player.running 
		and not btn(➡️)
		and not btn(⬅️)
		and not player.falling
		and not player.junmping then
			player.running=false
			player.sliding=true
	end

	if player.dy>0 then
		player.falling=true
		player.landed=false
		player.junmping=false

		if map_collide(player, "down", 0) then
			player.landed=true
			player.falling=false
			player.dy=0
			player.y-=((player.y+player.h+1)%8)-1
		end
	elseif player.dy<0 then
		player.junmping=true
		if map_collide(player, "up", 1) then
			player.dy=0
		end
	end

	if player.dx<0 then
		if map_collide(player, "left", 1) then
			player.dx=0
		end
	elseif player.dx>0 then
		if map_collide(player, "right", 1) then
			player.dx=0
		end
	end

	if player.sliding then
		if abs(player.dx) <.2
		or player.running then
			player.dx=0
			player.sliding=false
		end
	end

	if player.invulnerable and tiempo==80 then
		player.invulnerable=false
	end

	player.x+=player.dx
	player.y+=player.dy

	--limit player to map
	if player.x<map_start then
		player.x=map_start
	end
	if player.x>map_end-player.w then
		player.x=map_end-player.w
	end
end

function monster_update(e)
	e.dy+=gravity
	e.dx*=friccion
	e.running=true
	
	--este comentario indica que franccesco y santiago no hay tocado codigo aun
	-- se dice "han" troglodita

	--si el jugador esta vivio y no es invulnerable
	if player.vida>0 and not player.invulnerable then
		-- colision con el jugador
		if colision(e,player) then
			tiempo=0
			e.anim=e.sp_attack
			e.attack=true
			player.invulnerable=true
			player.vida-=1

			--pequeれねo salto
			if not player.junmping then
				player.dy-=(player.boost-1.5)
				player.landed=false
			end

			sfx(1)
		end
	end
	if tiempo==15 then
		e.attack=false
	end

	--acelera hacia adelante
	if e.flp then
		e.dx-=e.acc
	else
		e.dx+=e.acc
	end

	if e.dy>0 then
		e.falling=true
		e.landed=false
		e.junmping=false

		if map_collide(e, "down", 0) then
			e.landed=true
			e.falling=false
			e.dy=0
			e.y-=((e.y+e.h+1)%8)-1
		end
	elseif e.dy<0 then
		e.junmping=true
		if map_collide(e, "up", 1) then
			e.dy=0
		end
	end

	if e.dx<0 then
		if map_collide(e, "left", 1) or
			e.x<=map_start
		then
			e.x+=2
			e.dx=0
			e.flp=false
		end
	elseif e.dx>0 then
		if map_collide(e, "right", 1) or
			e.x>=map_end-e.w
			then
			e.x-=2
			e.dx=0
			e.flp=true
		end
	end

	e.x+=e.dx
	e.y+=e.dy

	if e.x<=map_start then
		e.x=map_start
		--e.flip = false
	end
	if e.x>map_end - e.w then
		e.x=map_end - e.w
		--e.flip = false
	end

end

function item_update(e)
	if colision(player, e)then
		if not e.recogido then
			player.cant_trash+=1
			player.score=player.cant_trash*e.score
			sfx(0)
		end
		e.recogido=true
	end
end

function carteles_update(c)
	if colision(player, c) then
		c.viendo=true
	else
		c.viendo=false
	end
end

