function map_collide(obj, dir, flag)
	local x=obj.x
	local y=obj.y
	local h=obj.h
	local w=obj.w

	local x1=0 local y1=0
	local x2=0 local y2=0

	if dir=="left" then
		x1=x+4 
		x2=x 
		y1=y
		y2=y+h-1
	elseif dir=="right" then
		x1=x+w-5 
		x2=x+w
		y1=y 
		y2=y+h-1
	elseif dir=="up" then
		x1=x+2
		x2=x+w
		y1=y-1
		y2=y
	elseif dir=="down" then
		x1=x+5
		x2=x+w
		y1=y+h
		y2=y+h
	end

	x1/=8
	x2/=8
	y1/=8
	y2/=8

	if fget(mget(x1,y1), flag)
		or fget(mget(x1,y2), flag)
		or fget(mget(x2,y1), flag)
		or fget(mget(x2,y2), flag) then
		return true
	else
		return false
	end
end

function colision(a, b)
	local box_a = abs_box(a)
	local box_b = abs_box(b)
		
	if box_a.w > box_b.x and
		box_a.x < box_b.w and
		box_a.y < box_b.h and
		box_a.h > box_b.y then
		return true
		end
	
	return false
end

function abs_box (e)
 local box = {}
 
	box.x = e.x			--x=izquierda
	box.w = e.x + e.w	--w=derecha
	box.y = e.y			--y=arriba
	box.h = e.y + e.h	--h=abajo
 
 	return box
end

