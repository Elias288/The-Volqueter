function new_entity(sp, x, y, flp)
	--sprite, x, y, ancho, halto, direccion
	local a = {
		sp=sp, 		--sprite actual
		anim=0,
		sp_attack=4,
		x=x, y=y,
		h=16, w=10,
		dx=0, dy=0,
		flp=flp, 	--false:derecha true:izquierda
		acc=0.5,  	--aceleracion
		boost=4,  	--salto
		vida=3, 	--vida
		time=0,
		
		golp=false,
		junmping=false,
		running=false,
		landed=false,
		falling=false,
		sliding=false,

		s=estados.idle,
		draw=draw_actor,
		move=move_actor,
	}

	if(fget(sp,7)) then
		a.is_=true
		a.acc=0.2
		a.move=monster_update
		a.attack=false
		add(enemigos, a)
	end
	add(entidades, a)
	return a
end

function make_player(sp, x, y)
	flp=false
	local a = new_entity(sp, x, y, flp)

	a.is_player=true		--es jugador
	a.carro=dibujar_carro
	a.w_carro=16
	a.c_carro=0				--capacidad carro
	a.anim_c=0 				--carro actual
	a.sp_c=33				--first srpite carro

	a.score=0
	a.invulnerable=false
	a.invT=1				--determina el tiempo de invulnerabilidad
	a.id=0

	return a
end

function new_item(sp, x, y, sc)
	local item={
		sp=sp,
		x=x, y=y,
		w=8, h=8,
		recogido=false,
		score=sc,
		update=item_update,
	}
	add(items, item)
	return item
end

function _init()
	gravity=0.3
	friccion=0.85

	tiempo=0

	player = make_player(1, 10, 48)
	
	local maxi = new_entity(192, 300, 48, true)
	local nancy = new_entity(224, 270, 48, true)

	--maxis
	for k,v in pairs(posiciones) do
		local npc = new_entity(192, v, 48, true)
 	end
	--[[nancys
	for k,v in pairs(posiciones) do
		local npc = new_entity(192, v, 48, true)
 	end]]

	--items
	for i=1,2 do
		local item=new_item(
				94, 
				posiciones_items[i][1], 
				posiciones_items[i][2], 
				5
			)
	end
	
	
	palt(0, false)
	palt(7, true)

	cam_x=0
	map_start=0
	map_end=1024

	music(0)
end

