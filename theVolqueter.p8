function draw_actor(a) 
	if a.vida>0 then
		sspr(
			((a.sp+a.anim)%16)*8,
			((a.sp+a.anim)\16)*8,
			a.w, --ancho
			a.h, --alto
			a.x, --xx
			a.y, --y
			a.w, -- no se ancho
			a.h, -- no se alto
			a.flp --flip
		)
	else
		spr(83,a.x,a.y+8)		
	end
end

function dibujar_carro(obj)
	local c_x, c_flp, t

	if obj.flp then --izquierda
		c_x = obj.x+10
	else	--derecha
		c_x = obj.x-16
	end

	if player.vida==3 then
		obj.sp_c=33
	elseif player.vida==2 then
		obj.sp_c=128
	else
		obj.sp_c=160
	end
	if player.invulnerable then
		t=4
	else
		t=player.invT
	end

	if tiempo%t==0 then
		sspr(
			((obj.sp_c+obj.anim_c)%16)*8,
			((obj.sp_c+obj.anim_c)\16)*8,
			obj.w_carro, --ancho
			obj.h, --alto
			c_x, --x
			obj.y, --y
			obj.w_carro, -- no se ancho
			obj.h, -- no se alto
			obj.flp --flip
		)
	end

	local score=player.score
	--basura en carro
	if score>=5 and score<10 then
		sspr(
			((132+obj.anim_c)%16)*8,
			((132+obj.anim_c)\16)*8,
			obj.w_carro, --ancho
			obj.h, --alto
			c_x, --x
			obj.y, --y
			obj.w_carro, -- no se ancho
			obj.h, -- no se alto
			obj.flp --flip
		)
	elseif score>=10 and score<20 then
		sspr(
			((136+obj.anim_c)%16)*8,
			((136+obj.anim_c)\16)*8,
			obj.w_carro, --ancho
			obj.h, --alto
			c_x, --x
			obj.y, --y
			obj.w_carro, -- no se ancho
			obj.h, -- no se alto
			obj.flp --flip
		)
	elseif score>=20 then
		sspr(
			((140+obj.anim_c)%16)*8,
			((140+obj.anim_c)\16)*8,
			obj.w_carro, --ancho
			obj.h, --alto
			c_x, --x
			obj.y, --y
			obj.w_carro, -- no se ancho
			obj.h, -- no se alto
			obj.flp --flip
		)
	end
end

